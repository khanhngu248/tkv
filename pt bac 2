#include<iostream>
#include<math.h>
#include<iomanip>
using namespace std;
int main()
{
	float x1,x2,delta,a,b,c;
	cin>>a>>b>>c;
	if(a<0)
	{
		delta=b*b-4*a*c;
		if(delta>0)
		{
			x1=((-b)-sqrt(delta))/2*a;
			x1=((-b)+sqrt(delta))/2*a;
		}
		if(delta<0)
		{
			cout<<"No solution.";
		}
		if(delta==0)
		{
			x1=x2=-b/(2*a);
		}
		
	}
	cout<<x1;
	cout<<x2;
	
}
